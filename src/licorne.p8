pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
p={x=30,y=20,speed=4}
cls()
palt(0,true)
end

function _update()
if (btn(➡️))p.x+=1
if (btn(⬅️))p.x-=1
if (btn(⬆️))p.y-=1
if (btn(⬇️))p.y+=1
end


function _draw()
cls()
map()
camera(p.x-30,p.y-30)
spr(64,p.x,p.y)
spr(67,60,60)
end
